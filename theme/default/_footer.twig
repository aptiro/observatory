            </div>
            {% set currentPath = path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) %}
            {% if currentPath == '/' %}
            <div class="col-sm-4 mapping-sidebar">
                {% set tags = tagCloud() %}
                {% for tag in tags %}
                    <a href="/tags/{{ tag.slug }}" style="font-size: {{ tag.size }}em;">
                        {{ tag.name }}
                    </a>
                {% endfor %}
            </div>
            {% endif %}
        </div> 
        <footer id="page-footer" role="navigation">
        </footer>   
    <footer class="clearfix">
        {{ menu('footer', '_menu_footer.twig') }}
    </footer>    
    </div>
<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.1/jquery.min.js"></script>
